    "ava": "^0.15.2",
    "coveralls": "^2.11.9",
    "expect": "^1.19.0",
    "gh-pages": "^0.11.0",
    "gitbook-cli": "^2.1.3",
    "nightmare": "^2.5.2",
    "nyc": "^6.4.4",
    "portfinder": "^1.0.3",
    "strip-ansi": "^3.0.1",
    "temp": "^0.8.3"
